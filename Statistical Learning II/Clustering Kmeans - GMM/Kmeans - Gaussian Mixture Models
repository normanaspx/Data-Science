        "## Kmeans"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "h1ThtijgJgYG"
      },
      "source": [
        "K = 7\n",
        "\n",
        "class point():\n",
        "    def __init__(self, data):\n",
        "        self.data = data\n",
        "        self.k = np.random.randint(0,K)\n",
        "    \n",
        "    def __repr__(self):\n",
        "        return str({\"data\":self.data, \"k\":self.k})\n"
      ],
      "execution_count": 15,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
